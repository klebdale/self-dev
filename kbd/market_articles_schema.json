{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "minItems": 5,
  "items": {
    "type": "object",
    "properties": {
      "article_title": {
        "type": "string"
      },
      "publisher": {
        "type": "string"
      },
      "date_published": {
        "type": "string",
        "format": "date"
      },
      "url": {
        "type": "string",
      },
      "author": {
        "type": "string"
      },
      "article_summary": {
        "type": "string"
      },
      "credibility_score": {
        "type": "number",
        "description": "A score representing the credibility of the article based on the publisher and the source."
      },
      "content_type": {
        "type": "string",
        "enum": ["news", "case study", "interview", "opinion", "report", "analysis"],
        "description": "The specific type of content the article represents."
      },
      "keywords": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "description": "Key topics or keywords covered in the article."
      }
    },
    "required": [
      "article_title",
      "publisher",
      "date_published",
      "url"
    ]
  }
}
